{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "WebWorker"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "next-env.d.ts",
    "typings",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/[locale]/expagetrans",
    ".next/types/**/*.ts",
    "types/**/*.ts",
    "vendor/Mp3LameEncoder.min.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
